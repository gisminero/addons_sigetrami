<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record model="ir.ui.view" id="exp_cant_pertenencias.exp_cant_pertenencias_over">
        <field name="name">Expediente pertenencias</field>
        <field name="model">exp_cant_pertenencias</field>
        <field name="inherit_id" ref="expediente.form"/>
        <field name="arch" type="xml">
          <xpath expr="/form/sheet/notebook/sheet/group/group/field[@name='observaciones']" position="before">
            <group col="2">
              <field name="pertenencias"/>
            </group>
          </xpath>
        </field>
    </record>
  </data>
</odoo>